programa {
  funcao inicio() {
    
      inteiro temperatura

      escreva("Qual a temperatura? \n")
         leia(temperatura)


      se(temperatura < 0){
        escreva("A temperatura é negativa")
      }
      
      se(temperatura > 0){
        escreva("A temperatura é positiva")
      }
       
       se(temperatura == 0){
        escreva("A temperatura é 0")
       }




  }
}
